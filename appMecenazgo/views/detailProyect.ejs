<%- include('./partials/head.ejs', {Titulo : 'Mecenazgo | Detalle Producto'}) -%> 

<body>

  <%- include('./partials/header') -%> 

<div class="contenedor-mio">


  <div class="" style="justify-content: center;">

    <div class="otro-contenedor-cards">

        <div class="card" style="text-align: center;">
        
        <div class="card-body">
          
          <h5 class="card-title" style="margin-bottom: 10%;"> 
            <%=projects.title%> </h5>
            
          <h6 class="card-subtitle mb-2 text-muted"> <%= projects.sub_title %>  </h6>
          <p class="card-text" style="margin-bottom: 10%;"> <%= projects.description %>  </p>
          
          <div class="progress" style="margin-bottom: 10%;">
            <div class="progress-bar" role="progressbar" style="width: 25%;" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">25%</div>
          </div>
          <a href="#" class="card-link"><button type="button" class="btn btn-success">Aportar al proyecto</button></a>
          
          <a href="http://localhost:3000/project/updateProyect<%= projects.id %>">
            <button type="button" class="btn btn-outline-success">
              <i class="bi bi-pencil-square"></i>
            </button></a>

          <a href="">
            <button type="button" class="btn btn-outline-danger">
              <i class="bi bi-trash"></i>
            </button></a>
          
          
          
        </div>

      </div>


    </div>

  </div>
</div>
  <%- include('./partials/footer') -%> 

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf"
    crossorigin="anonymous"></script>
</body>

</html>